PORT=3000
NODE_ENV=development

# Local MongoDB Connection (for development)
MONGODB_URI=mongodb://127.0.0.1:27017/memes_db

# MongoDB Atlas Connection (for production)
MONGODB_ATLAS_URI=mongodb+srv://<username>:<password>@<cluster>.mongodb.net/memes_db?retryWrites=true&w=majority

# Session Secret (change this to a random string)
SESSION_SECRET=your_super_secret_session_key

# Vercel and Render URLs (for production)
VERCEL_URL=https://your-app-name.vercel.app
RENDER_URL=https://your-app-name.onrender.com 